{
  "name": "ms-prestamos-webhook",
  "version": "1.0.0",
  "description": "Microservicio webhook para acceso a informaci√≥n de clientes con arquitectura onion",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "pnpm run prisma:generate:all",
    "prisma:generate:postgres": "prisma generate --schema=prisma/postgres/schema.prisma",
    "prisma:generate:sqlserver": "prisma generate --schema=prisma/sqlserver/schema.prisma",
    "prisma:generate:all": "pnpm run prisma:generate:postgres && pnpm run prisma:generate:sqlserver",
    "prisma:migrate:postgres": "prisma migrate dev --schema=prisma/postgres/schema.prisma",
    "prisma:migrate:sqlserver": "prisma migrate dev --schema=prisma/sqlserver/schema.prisma",
    "prisma:studio:postgres": "prisma studio --schema=prisma/postgres/schema.prisma",
    "prisma:studio:sqlserver": "prisma studio --schema=prisma/sqlserver/schema.prisma"
  },
  "keywords": [
    "webhook",
    "typescript",
    "express",
    "prisma",
    "onion-architecture"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.29.1",
    "@prisma/client": "^5.7.1",
    "@sentry/node": "^10.9.0",
    "@types/pdfkit": "^0.17.3",
    "@types/xlsx": "^0.0.36",
    "canvas": "3.2.0",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "pdfkit": "^0.17.2",
    "pdfmake": "^0.2.20",
    "xlsx": "^0.18.5",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pdfmake": "^0.2.12",
    "node-gyp": "^11.5.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}